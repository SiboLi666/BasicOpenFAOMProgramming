FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    beta
    {
        solver          GAMG;//PCG;
        smoother  DIC;
        tolerance       1e-06;
        relTol          0;
    }
}

